{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "Bonsai.ZeroMQ/Bonsai.ZeroMQ.csproj"
          ],
          "src": "../src"
        }
      ],
      "dest": "api",
      "filter": "filter.yml",
      "enumSortOrder": "declaringOrder",
      "memberLayout": "separatePages"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      },
      {
        "exclude": [
          "_site/**",
          "filter.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "workflows/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "globalMetadata": {
      "_appName": "Bonsai - ZeroMQ",
      "_appTitle": "Bonsai.ZeroMQ",
      "_appFooter": "&copy; 2024 Bonsai Foundation CIC and Contributors. Made with <a href=\"https://dotnet.github.io/docfx\">docfx</a>",
      "_enableNewTab": true,
      "_enableSearch": true,
      "_gitContribute": {
        "repo": "https://github.com/bonsai-rx/zeromq",
        "branch": "main",
        "apiSpecFolder": "docs/apidoc"
      }
    },
    "output": "_site",
    "template": [
      "default",
      "modern",
      "bonsai/template",
      "template"
    ],
    "markdownEngineName": "markdig",
    "markdownEngineProperties": {
      "markdigExtensions": [
        "attributes",
        "customcontainers"
      ]
    },
    "xref": [
      "https://bonsai-rx.org/docs/xrefmap.yml",
      "https://horizongir.github.io/reactive/xrefmap.yml"
    ]
  }
}